<VBox stylesheets="@../styles/Styles.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.tamusa.sis.edi.transit.CompareController">
   <children>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="800.0">
               <children>
                  <Label text="Input FIle" />
               </children>
            </HBox>
            <HBox alignment="CENTER" layoutX="10.0" layoutY="10.0" prefHeight="100.0" prefWidth="800.0">
               <children>
                  <Label text="Output File" />
               </children>
            </HBox>
         </children>
      </HBox>
      <AnchorPane id="AnchorPane" prefHeight="1000.0" prefWidth="1600.0">
         <children>
            <SplitPane dividerPositions="0.5" layoutX="143.0" layoutY="209.0" prefHeight="160.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
